    "scripts": {
        "tslint:run": "tslint --fix -p .",
        "tslint:check": "tslint -p .",
        "tsc:check": "tsc --noEmit",
        "start": "cross-env NODE_ENV='development' babel-node ./server.js --mode development",
        "build": "cross-env NODE_ENV='production' webpack --mode production --colors --display-error-details --config webpack.conf.js",
        "test:coverage": "cross-env NODE_ENV='testing' jest --verbose --no-cache --coverage",
        "test:watch": "cross-env NODE_ENV='testing' jest --verbose --no-cache --watchAll",
        "storybook:start": "start-storybook -p 6006",
        "storybook:build": "build-storybook -o dist_storybook",
        "storybook:docker:build": "docker build ./ -t storybook_dev -f Dockerfile.storybook",
        "storybook:docker:start": "docker run -v ${PROJECT_PATH}/src:/react/src -v ${PROJECT_PATH}/stories:/react/stories -p 6006:6006 storybook_dev",
        "prettier:run": "prettier --write \"src/**/*.ts*\"",
        "prettier:check": "prettier -l \"src/**/*.ts*\""
